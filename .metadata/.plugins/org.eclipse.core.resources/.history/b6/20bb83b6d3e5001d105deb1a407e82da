public class BikeBlocks {

	String name;
	int price;
	{
		this.name="Himalayan";
		this.price=200000;
		System.out.println(this.name+""+this.price);
	}
	public static void main(String[] args) {

		BikeBlocks b1=new BikeBlocks();
		BikeBlocks b2=new BikeBlocks();
		System.out.println(b1.toString());
		System.out.println(b2.toString());




	}







}
