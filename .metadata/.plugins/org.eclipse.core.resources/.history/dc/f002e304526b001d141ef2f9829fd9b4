
public class SwitchCase {

	public static void main(String[] args) {
		
 
		int n1=223;
		int n2=344;
		int option=  case 1    ;
		 
		switch(option)
		
		{
		case 1:
		{ 
			System.out.println(n1+n2);
			
		}
		break;
		case 2:
		{
			System.out.println(n1-n2);
			
		}
		break;
		case 3:
		{ 
			System.out.println(n1*n2);
		}
		break;
		case 4:
		{ 
			System.out.println(n1/n2);
		}
		
		}
		break;
		
		
		{
			System.out.println("invalid data");
		}
		break;
	
		}
}
